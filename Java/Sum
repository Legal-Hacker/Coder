
class Main {
    public static void main(String[] args) {
        int a=10;
        int b=20;
        int sum=a+b;
        System.out.print(sum);
    }
}

2nd Way

class Main {
    public static void main(String[] args) {
        int a=10,b=20;;
        int sum=a+b;
        System.out.print(sum);
    }
}
